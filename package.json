{
  "name": "badmr",
  "version": "1.0.0",
  "main": "app.js",
  "private": true,
  "dependencies": {
    "bluebird": "^3.7.2",
    "concurrently": "^9.2.1",
    "connect-ensure-login": "^0.1.1",
    "connect-sqlite3": "^0.9.16",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csurf": "^1.2.2",
    "db-migrate": "^0.11.14",
    "db-migrate-sqlite3": "^1.0.2",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-promise-json-router": "^1.0.8",
    "express-session": "^1.17.2",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.11",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "sqlite3": "^5.1.7",
    "use-strict": "^1.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "copy-dir": "^1.3.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "npm": ">=11.6.3",
    "node": ">=25.2.0"
  },
  "homepage": "https://github.com/vasilevichra/badmr",
  "repository": {
    "type": "git",
    "url": "git://github.com/vasilevichra/badmr.git"
  },
  "bugs": {
    "url": "https://github.com/vasilevichra/badmr/issues"
  },
  "scripts": {
    "update": "npm outdated -p | cut -d ':' -f 4 | xargs npm install",
    "migrate": "node node_modules/db-migrate/bin/db-migrate up",
    "compile": "export NODE_ENV=dev && nodemon src/bin/start.js",
    "open": "concurrently \"npm run compile\" \"sleep 1 && open http://localhost:3000/\"",
    "test": "(rm -f test.db || true) && node node_modules/db-migrate/bin/db-migrate up -e test -c 4 && export NODE_ENV=test && node --test",
    "dev": "npm run test && (rm -f badmr.sqlite3 || true) && npm run migrate && export NODE_ENV=dev && npm run open",
    "prod": "npm install && npm run migrate && npm run test && export NODE_ENV=prod && node src/bin/start.js",
    "release": "rsync --info=progress2 --ignore-times --delete --delete-after --recursive --exclude-from='/Users/r/Desktop/badmr/.gitignore' /Users/r/Desktop/badmr box:~/ && ssh box 'sudo /home/r/badmr/etc/scripts/release.sh'"
  },
  "author": {
    "name": "Vasilevich Roman",
    "email": "vasilevich@gmail.com",
    "url": "https://t.me/vasilevichr"
  },
  "license": "MIT",
  "description": "Бадминтон и ром"
}
