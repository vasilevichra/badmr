{
  "name": "badmr",
  "version": "1.0.0",
  "main": "app.js",
  "private": true,
  "dependencies": {
    "bluebird": "^3.7.2",
    "concurrently": "^9.1.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-sqlite3": "^1.0.2",
    "express": "^4.21.2",
    "express-promise-json-router": "^1.0.8",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.9",
    "sqlite3": "^5.1.7",
    "use-strict": "^1.0.1"
  },
  "devDependencies": {
    "copy-dir": "^1.3.0",
